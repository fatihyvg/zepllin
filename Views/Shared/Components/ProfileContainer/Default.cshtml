<div class="container is-widescreen is-fullhd" style="margin-top:120px;">
    <div id="profile-gallery" class="justified-gallery">
        @foreach (var item in Model)
        {
        <a href="@Url.Action("Show","Image",new { ImageId = item.ImageId,ImageTitle = UrlSeo.ConvertToSeoUrl(item.ImageTitle) })" rel="nofollow" title="@item.ImageWithoutSeoTitle">
            <img alt="@item.ImageTitle" title="@item.ImageWithoutSeoTitle" src="@Url.Content(BaseUrl.baseurl+item.ImageSrc)" />
            <div class="jg-caption">
                <h2>@item.ImageWithoutSeoTitle</h2>
            </div>
        </a>
       }
    </div>
</div>




